{
    "manifest_version": 2,
    "name": "Goodday",
    "version":"1",
 
    "chrome_url_overrides":
    {
        "newtab": "index.html"
    },
    "background":{
        "scripts": ["background.js", "javascript.js"]
    },
    "browser_action": {
        "default_icon": "good.png",
        "default_popup": "pong/pong.html",
        "default_title" : "Play pong!!"
    },
    "permissions": [
        "tabs"
    ],
    "web_accessible_resources": ["img/*.png"]
}

