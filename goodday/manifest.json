{
    "manifest_version": 2,
    "name": "Goodday",
    "version":"1",
 
    "chrome_url_overrides":
    {
        "newtab": "index.html"
    },
    "background":{
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "good.jpg",
        "default_popup": "popup.html",
        "default_title" : "A popup will come here."
    },
    "permissions": [
        "tabs"
    ]
}